{
  "name": "digitalhub-frontend",
  "version": "0.1.2",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel:build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "format": "prettier --write .",
    "stylecheck": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "test": "playwright test",
    "db:dev:up": "docker-compose down && docker-compose up db",
    "db:dev:seed": "dotenv -c development -- tsx prisma/scripts/seed.ts",
    "db:dev:migrate": "dotenv -c development -- npx prisma migrate dev",
    "db:dev:types": "dotenv -c development -- npx prisma generate",
    "db:prod:shell": "dotenv -c development -- bash -c 'PGPASSWORD=${PROD_DBPASS} psql -h ${PROD_DBHOST} -p ${PROD_DBPORT} -U ${PROD_DBUSER} -d ${PROD_DBNAME}'",
    "db:prod:kill-sessions": "dotenv -c development -- bash -c 'DATABASE_NAME=${PROD_DBNAME} ./scripts/db/kill-sessions.sh'",
    "db:preview:kill-sessions": "dotenv -c development -- bash -c 'DATABASE_NAME=${PROD_PREVIEWDBNAME} ./scripts/db/kill-sessions.sh'",
    "db:preview:drop": "dotenv -c development -- bash -c 'PGPASSWORD=${PROD_DBPASS} psql -h ${PROD_DBHOST} -p ${PROD_DBPORT} -U ${PROD_DBUSER} -d ${PROD_DBNAME} -c \"DROP DATABASE ${PROD_PREVIEWDBNAME};\"'",
    "db:preview:create": "dotenv -c development -- bash -c 'PGPASSWORD=${PROD_DBPASS} psql -h ${PROD_DBHOST} -p ${PROD_DBPORT} -U ${PROD_DBUSER} -d ${PROD_DBNAME} -c \"CREATE DATABASE ${PROD_PREVIEWDBNAME} WITH TEMPLATE ${PROD_DBNAME} OWNER ${PROD_DBUSER};\"'",
    "db:preview:reset": "npm run db:preview:kill-sessions && npm run db:preview:drop && npm run db:prod:kill-sessions && npm run db:preview:create",
    "email:dev": "email dev -p 4444"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.421.0",
    "@aws-sdk/client-ses": "^3.421.0",
    "@aws-sdk/lib-storage": "^3.421.0",
    "@hookform/resolvers": "^3.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.4.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@react-email/components": "^0.0.7",
    "@sentry/nextjs": "^7.86.0",
    "@tanstack/react-table": "^8.10.1",
    "aws-crt": "^1.19.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "csv-parse": "^5.5.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "eslint": "8.48.0",
    "framer-motion": "^10.16.4",
    "libphonenumber-js": "^1.10.49",
    "lucide-react": "^0.279.0",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-email": "^1.9.4",
    "react-hook-form": "^7.46.2",
    "recharts": "^2.8.0",
    "sharp": "^0.32.6",
    "tailwind-merge": "^1.14.0",
    "typeid-js": "^0.3.0",
    "zod": "^3.22.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "^1.38.1",
    "@tailwindcss/forms": "^0.5.6",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "prisma": "^5.4.1",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  }
}
