    "@playwright/test": "^1.56.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^2.1.4",
