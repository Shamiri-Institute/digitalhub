    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.13.2",
    "aws-crt": "^1.19.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "eslint": "^9",
    "fast-csv": "^5.0.0",
    "framer-motion": "^12.24.7",
    "googleapis": "^131.0.0",
    "gsap": "^3.12.5",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.12.33",
    "lucide-react": "^0.525.0",
    "mime-types": "^2.1.35",
    "next": "16.0.10",
