{
  "name": "digitalhub-frontend",
  "version": "0.1.5",
  "private": true,
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel:build": "prisma generate && prisma migrate deploy && next build",
    "vercel:testing:build": "prisma generate && prisma migrate reset --force && prisma migrate deploy && npm run db:seed && next build",
    "vercel:training:build": "prisma generate && prisma migrate reset --force && prisma migrate deploy && npm run db:seed && next build",
    "start": "next start",
    "format": "prettier --write .",
    "stylecheck": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "test:ci": "playwright test",
    "test:dev": "dotenv -c development -- playwright test",
    "test:dev:only": "dotenv -c development -- playwright test --ui tests/e2e/hub-coordinator/schools-view.spec.ts",
    "test:dev:ui": "dotenv -c development -- playwright test --ui",
    "test:db:dump": "docker-compose exec db pg_dump --data-only -U youth -d shamiri_db -f /var/lib/postgresql/data/dump.sql && docker cp $(docker-compose ps -q db):/var/lib/postgresql/data/dump.sql ./tests/fixtures/dump.sql",
    "test:db:load": "psql -d shamiri_db -h localhost -p 5432 -f ./tests/fixtures/dump.sql",
    "test:unit": "dotenv -c development -- vitest",
    "next-auth:session": "dotenv -c development -- tsx scripts/auth/get-next-session.ts",
    "db:dev:up": "docker-compose down && docker-compose up db",
    "db:dev:seed": "dotenv -c development -- tsx prisma/scripts/seed.ts",
    "db:seed": "dotenv -c development -- tsx prisma/scripts/seed-v2.ts",
    "db:dev:migrate": "dotenv -c development -- npx prisma migrate dev",
    "db:dev:types": "dotenv -c development -- npx prisma generate",
    "db:prod:shell": "dotenv -c development -- bash -c 'PGPASSWORD=${PROD_DBPASS} psql -h ${PROD_DBHOST} -p ${PROD_DBPORT} -U ${PROD_DBUSER} -d ${PROD_DBNAME}'",
    "db:prod:kill-sessions": "dotenv -c development -- bash -c 'DATABASE_NAME=${PROD_DBNAME} ./scripts/db/kill-sessions.sh'",
    "db:preview:kill-sessions": "dotenv -c development -- bash -c 'DATABASE_NAME=${PROD_PREVIEWDBNAME} ./scripts/db/kill-sessions.sh'",
    "db:preview:drop": "dotenv -c development -- bash -c 'PGPASSWORD=${PROD_DBPASS} psql -h ${PROD_DBHOST} -p ${PROD_DBPORT} -U ${PROD_DBUSER} -d ${PROD_DBNAME} -c \"DROP DATABASE ${PROD_PREVIEWDBNAME};\"'",
    "db:preview:create": "dotenv -c development -- bash -c 'PGPASSWORD=${PROD_DBPASS} psql -h ${PROD_DBHOST} -p ${PROD_DBPORT} -U ${PROD_DBUSER} -d ${PROD_DBNAME} -c \"CREATE DATABASE ${PROD_PREVIEWDBNAME} WITH TEMPLATE ${PROD_DBNAME} OWNER ${PROD_DBUSER};\"'",
    "db:preview:reset": "npm run db:preview:kill-sessions && npm run db:preview:drop && npm run db:prod:kill-sessions && npm run db:preview:create",
    "db:dump:local": "dotenv -c development -- ./scripts/db/dump-db.sh",
    "email:dev": "email dev -p 4444",
    "release": "commit-and-tag-version"
  },
  "commit-and-tag-version": {
    "scripts": {
      "precommit": "npm run format > /dev/null"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.663.0",
    "@aws-sdk/client-ses": "^3.750.0",
    "@aws-sdk/lib-storage": "^3.421.0",
    "@google-cloud/local-auth": "^3.0.1",
    "@gsap/react": "^2.1.1",
    "@hookform/resolvers": "^3.9.1",
    "@internationalized/date": "^3.5.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@react-email/components": "^0.0.33",
    "@react-email/render": "1.0.5",
    "@sentry/nextjs": "^9.14.0",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.13.2",
    "@web-std/file": "^3.0.3",
    "aws-crt": "^1.19.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^0.2.0",
    "csv-parse": "^5.5.3",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "eslint": "8.48.0",
    "fast-csv": "^5.0.0",
    "framer-motion": "^10.16.4",
    "google-auth-library": "^9.4.2",
    "googleapis": "^131.0.0",
    "gsap": "^3.12.5",
    "libphonenumber-js": "^1.10.49",
    "lucide-react": "^0.279.0",
    "mime-types": "^2.1.35",
    "next": "^14.2.23",
    "next-auth": "^4.24.10",
    "next-s3-upload": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-aria-components": "^1.1.1",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-email": "3.0.7",
    "react-hook-form": "^7.53.2",
    "react-stately": "^3.30.1",
    "recharts": "^2.8.0",
    "sharp": "^0.32.6",
    "tailwind-merge": "^1.14.0",
    "tsx": "^4.7.0",
    "typeid-js": "^0.3.0",
    "zod": "^3.23.8",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@playwright/test": "^1.38.1",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/react": "^14.1.2",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.17.6",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "commit-and-tag-version": "^12.5.1",
    "dotenv-cli": "^7.3.0",
    "eslint-config-next": "^14.2.1",
    "eslint-config-prettier": "^9.0.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3",
    "vitest": "^1.6.1"
  }
}
